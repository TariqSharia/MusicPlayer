        <!--<meta-data-->
            <!--android:name="android.support.PARENT_ACTIVITY"-->
            <!--android:value=".MainActivity" />-->
        <activity android:name=".YaSamtActivity"
            android:label="Ya Samt"
            android:parentActivityName=".AlbumsActivity"></activity>
        <!--<meta-data-->
            <!--android:name="android.support.PARENT_ACTIVITY"-->
            <!--android:value=".AlbumsActivity" />-->
        <activity android:name=".UnknownAlbumActivity"
            android:label="Unkown"
            android:parentActivityName=".AlbumsActivity"></activity>
